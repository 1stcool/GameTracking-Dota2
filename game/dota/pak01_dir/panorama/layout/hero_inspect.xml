		<Panel id="BattlePassHeroUpsell">
			<Panel class="HeaderRow">
				<Panel class="BattlePassIcon"/>
				<Panel class="DataTitlesContainer">
					<Label class="DataTitle" text="#DOTA_BattlePass_FriendsAndFoes"/>
					<Label class="DataSubtitle" text="#DOTA_BattlePass_BasedOnMatchup"/>
				</Panel>
				<Panel class="SummaryContainer">
					<!--<Label class="DataNumberLarge Outcome" text="{s:summary_result}"/>-->
					<!--<Label class="DataSubtitle" text="{s:summary_text}"/>-->
					<Panel class="InfoIcon"  onmouseout="UIHideTextTooltip();" onmouseover="UIShowTextTooltip( #DOTA_BattlePass_HeroPerformance_Tooltip );" />
				</Panel>
			</Panel>
            <Panel id="UpsellMessaging">
                <Label id="UpsellText" text="#DOTA_BattlePass_HeroPerformance_Upsell"/>
                <Label id="BattlePassText" text="#DOTA_BattlePass_HeroPerformance_UpsellBP" />
            </Panel>
		</Panel>
		<Panel id="BattlePassHeroData">
			<Panel class="HeaderRow">
				<Panel class="BattlePassIcon"/>
				<Panel class="DataTitlesContainer">
					<Label class="DataTitle" text="#DOTA_BattlePass_FriendsAndFoes"/>
					<Label class="DataSubtitle" text="#DOTA_BattlePass_BasedOnMatchup"/>
				</Panel>
				<Panel class="SummaryContainer">
					<!--<Label class="DataNumberLarge Outcome" text="{s:summary_result}"/>-->
					<!--<Label class="DataSubtitle" text="{s:summary_text}"/>-->
					<Panel class="InfoIcon"  onmouseout="UIHideTextTooltip();" onmouseover="UIShowTextTooltip( #DOTA_BattlePass_HeroPerformance_Tooltip );" />
				</Panel>
			</Panel>
			<Panel class="AlliesContainer">
				<Label class="DataSubtitle Vertical" text="#DOTA_BattlePass_MatchUp_Allies"/>
				<Panel class="AlliesRow">
					<Panel id="PlayerHero" class="HeroWinRateInfo">
						<DOTAHeroImage id="PlayerHeroImage" heroimagestyle="icon"/>
						<Label class="DataNumber You" text="#DOTA_BattlePass_MatchUp_You"/>
					</Panel>
					<Panel id="WinRateAlly1" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconAlly1"  heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_ally_1}"/>
					</Panel>
					<Panel id="WinRateAlly2" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconAlly2"  heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_ally_2}"/>
					</Panel>
					<Panel id="WinRateAlly3" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconAlly3" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_ally_3}"/>
					</Panel>
					<Panel id="WinRateAlly4" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconAlly4" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_ally_4}"/>
					</Panel>
				</Panel>
			</Panel>
			<Panel class="EnemiesContainer">
				<Label class="DataSubtitle Vertical" text="#DOTA_BattlePass_MatchUp_Enemies"/>
				<Panel class="EnemiesRow">
					<Panel id="WinRateEnemy1" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconEnemy1" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_enemy_1}"/>
					</Panel>
					<Panel id="WinRateEnemy2" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconEnemy2" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_enemy_2}"/>
					</Panel>
					<Panel id="WinRateEnemy3" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconEnemy3" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_enemy_3}"/>
					</Panel>
					<Panel id="WinRateEnemy4" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconEnemy4" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_enemy_4}"/>
					</Panel>
					<Panel id="WinRateEnemy5" class="HeroWinRateInfo">
						<DOTAHeroImage id="HeroIconEnemy5" defaultsrc="s2r://panorama/images/hero_selection/unknown_hero_icon_png.vtex" heroimagestyle="icon"/>
						<Panel class="WinRateArrow"/>
						<Label class="DataNumber" text="{s:win_rate_enemy_5}"/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<!--<Label id="HeroHype" text="{s:inspect_hero_hype}" /> -->
		<Label id="BanDescription" class="PickDesc" text="#DOTA_HeroInspect_BanDescription" />
		<Label id="PickDescription" class="PickDesc" text="#DOTA_HeroInspect_PickDescription" />
	</DOTAHeroInspect>

</root>
