<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_ping_wheel.vcss_c" />
	</styles>
	<DOTAPingWheel class="Hidden" hittest="false">
		<Panel id="Root">
			<Panel id="BG" class="PingWheelBG" />
			<Panel id="Attack" class="PingWheelOption" ping-option="attack"/>
			<Panel id="Retreat" class="PingWheelOption" ping-option="retreat" />
			<Panel id="Waypoint" class="PingWheelOption" ping-option="waypoint" />
			<Panel id="EnemyWard" class="PingWheelOption" ping-option="enemyward" />
			<Panel id="FriendlyWard" class="PingWheelOption" ping-option="friendlyward" />
			<Panel id="DefaultPing" class="PingWheelOption" ping-option="default" />
		</Panel>
	</DOTAPingWheel>
</root>


