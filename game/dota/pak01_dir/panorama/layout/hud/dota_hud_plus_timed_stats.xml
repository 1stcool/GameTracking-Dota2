<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_plus_timed_stats.vcss_c" />
	</styles>
	<DOTAPlusTimedStats>
		<Label class="TimedStatsHeader" text="Hero Performance"/>
		<Panel id="StatsContainer" hittestchildren="false" hittest="false">
			<Panel id="LabelColumn" class="DataColumn">
				<Label class="StatHeader" text="Stat"/>
	 <!--                                        
				                                        
				                                          -->
				<Label class="StatValue" text="LH"/>
				<Label class="StatValue" text="D"/>
				<Label class="StatValue" text="NW"/>
			</Panel>
			<Panel id="CurrentColumn" class="DataColumn">
				<Label class="StatHeader" text="Current" />
	 <!--                                            
				                                            
				                                              -->
				<Label class="StatValue" text="{d:last_hits}"/>
				<Label class="StatValue" text="{d:denies}"/>
				<Label class="StatValue" text="{d:networth}"/>
			</Panel>
			<Panel id="HistoricColumn" class="DataColumn">
				<Label class="StatHeader" text="Historic" />
	 <!--                                                   
				                                                   
				                                                     -->
				<Label class="StatValue" text="{d:target_lasthits}"/>
				<Label class="StatValue" text="{d:target_denies}"/>
				<Label class="StatValue" text="{d:target_networth}"/>
			</Panel>
			<Panel id="ArrowColumn" class="DataColumn">
				<Label class="StatHeader" text=""/>
	 <!--                                     
				                                     
				                                       -->
				<Panel class="PlusLastHits StatValue"/>
				<Panel class="PlusDenies StatValue"/>
				<Panel class="PlusNetWorth StatValue"/>
			</Panel>


			<!--
			                    
				                               
					                                                                                     
					                                                      
					        
				        
			        
			                   
				                               
					                                                                                    
					                                                      
					        
				        
			        

			                      
				                            
					                                             
				        
			        
		-->
		</Panel>
	</DOTAPlusTimedStats>
</root>
