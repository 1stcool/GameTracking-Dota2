		<Label id="Phrase5" class="Phrase" text="{s:phrase6_sound_icon}{s:phrase6_all_prefix}{s:phrase6}" html="true" />
		<Label id="Phrase6" class="Phrase" text="{s:phrase7_sound_icon}{s:phrase7_all_prefix}{s:phrase7}" html="true" />
		<Label id="Phrase7" class="Phrase" text="{s:phrase8_sound_icon}{s:phrase8_all_prefix}{s:phrase8}" html="true" />
	</DOTAChatWheel>
</root>


