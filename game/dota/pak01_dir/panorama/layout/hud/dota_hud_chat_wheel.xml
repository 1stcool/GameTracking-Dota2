			<Panel id="Arrow5" class="Arrow Hidden" />
			<Panel id="Arrow6" class="Arrow Hidden" />
			<Panel id="Arrow7" class="Arrow Hidden" />
			<Panel id="PointerContainer">
				<Panel id="WheelPointer" />
			</Panel>
			<Panel id="WheelBG">
				<Panel id="WheelCenter">
					<Panel id="WheelBG2">

					</Panel>
				</Panel>
			</Panel>
			<Panel id="Bubble" />
		</Panel>
		<Label id="Phrase0" class="Phrase" text="{s:phrase1_sound_icon}{s:phrase1_all_prefix}{s:phrase1}" html="true" />
