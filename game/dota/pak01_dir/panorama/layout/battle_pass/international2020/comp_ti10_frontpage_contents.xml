<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2020/bp_international2020_shared.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/international2020/comp_ti10_frontpage_contents.vcss_c" />
	</styles>

	<CustomLayoutPanel class="TI10FrontpageContents">
		<Panel class="ContentsSection Header">
			<Panel class="GroupStagePanel">
				<Label class="InfoSubText" text="#DOTA_Compendium_GroupStage"/>
				<Label class="InfoMainText" html="true" text="#DOTA_Compendium_GroupStageDates_Short"/>
			</Panel>
			
			<Panel class="TILogo" />

			<Panel class="MainEventPanel">
				<Label class="InfoSubText" text="#DOTA_Compendium_MainEvent"/>
				<Label class="InfoMainText" html="true" text="#DOTA_Compendium_MainEventDates_Short"/>
			</Panel>
		</Panel>
		
		<Panel class="MainContents">
			<Panel class="CenterContents VideoLive">
				<Panel class="VideoCenterPanel">
					<Panel class="TeamsFlank">
						<DOTATeamImage id="Team1Logo" class="TeamLogo" teamid="39" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(39, 1, 1)"/>
						<DOTATeamImage id="Team2Logo" class="TeamLogo" teamid="15" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(15, 5, 1)"/>
						<DOTATeamImage id="Team3Logo" class="TeamLogo" teamid="1883502" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(1883502, 4, 1)"/>
						<DOTATeamImage id="Team4Logo" class="TeamLogo" teamid="7390454" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7390454, 1, 1)"/>
						<DOTATeamImage id="Team5Logo" class="TeamLogo" teamid="5" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(5, 5, 1)"/>
						<DOTATeamImage id="Team6Logo" class="TeamLogo" teamid="8214850" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8214850, 6, 1)"/>
						<DOTATeamImage id="Team7Logo" class="TeamLogo" teamid="726228" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(726228, 5, 1)"/>
						<DOTATeamImage id="Team8Logo" class="TeamLogo" teamid="1838315" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(1838315, 3, 1)"/>
						<DOTATeamImage id="Team9Logo" class="TeamLogo" teamid="6209166" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(6209166, 4, 1)"/>
					</Panel>
					<Panel class="LiveVideoContainer">
						<Label text="Live Video" />
					</Panel>
					<Panel class="TeamsFlank TeamsFlankRight">
						<DOTATeamImage id="Team10Logo" class="TeamLogo" teamid="111474" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(111474, 3, 1)"/>
						<DOTATeamImage id="Team11Logo" class="TeamLogo" teamid="8254400" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8254400, 2, 1)"/>
						<DOTATeamImage id="Team12Logo" class="TeamLogo" teamid="7391077" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7391077, 2, 1)"/>
						<DOTATeamImage id="Team13Logo" class="TeamLogo" teamid="8260983" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8260983, 1, 1)"/>
						<DOTATeamImage id="Team14Logo" class="TeamLogo" teamid="8255756" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8255756, 2, 1)"/>
						<DOTATeamImage id="Team15Logo" class="TeamLogo" teamid="2586976" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(2586976, 3, 1)"/>
						<DOTATeamImage id="Team16Logo" class="TeamLogo" teamid="7119388" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7119388, 4, 1)"/>
						<DOTATeamImage id="Team17Logo" class="TeamLogo" teamid="8204512" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8204512, 5, 1)"/>
						<DOTATeamImage id="Team18Logo" class="TeamLogo" teamid="350190" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(350190, 6, 1)"/>
					</Panel>
				</Panel>
				<Panel class="ScheduleSection">
					<DOTACompTI10ScheduleWidget id="ScheduleWidget" />
				</Panel>
			</Panel>

			<Panel class="CenterContents VideoHidden">
				<Label class="InfoMainText TI10Tagline" text="#DOTA_Compendium_TI10Tagline"/>
				<Panel class="AegisContainer">
					<Panel class="TeamsFlankAegis">
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team1Logo" class="TeamLogo" teamid="39" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(39, 1, 1)"/>
							<DOTATeamImage id="Team2Logo" class="TeamLogo" teamid="15" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(15, 5, 1)"/>
							<DOTATeamImage id="Team3Logo" class="TeamLogo" teamid="1883502" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(1883502, 4, 1)"/>
						</Panel>
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team4Logo" class="TeamLogo" teamid="7390454" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7390454, 1, 1)"/>
							<DOTATeamImage id="Team5Logo" class="TeamLogo" teamid="5" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(5, 5, 1)"/>
							<DOTATeamImage id="Team6Logo" class="TeamLogo" teamid="8214850" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8214850, 6, 1)"/>
						</Panel>
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team7Logo" class="TeamLogo" teamid="726228" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(726228, 5, 1)"/>
							<DOTATeamImage id="Team8Logo" class="TeamLogo" teamid="1838315" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(1838315, 3, 1)"/>
							<DOTATeamImage id="Team9Logo" class="TeamLogo" teamid="6209166" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(6209166, 4, 1)"/>
						</Panel>
					</Panel>

					<Panel id="AegisImage" hittest="false" hittestchildren="false">
						<Panel class="Aegis" />
						<Panel class="AegisScreen" />
					</Panel>
					<Button class="AegisButton" onactivate="DOTAShowTI10CompHome()" onmouseover="AddStyle( AegisImage, Hovered )" onmouseout="RemoveStyle( AegisImage, Hovered )"/>

					<Panel class="TeamsFlankAegis TeamsFlankRight">
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team10Logo" class="TeamLogo" teamid="111474" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(111474, 3, 1)"/>
							<DOTATeamImage id="Team11Logo" class="TeamLogo" teamid="8254400" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8254400, 2, 1)"/>
							<DOTATeamImage id="Team12Logo" class="TeamLogo" teamid="7391077" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7391077, 2, 1)"/>
						</Panel>
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team13Logo" class="TeamLogo" teamid="8260983" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8260983, 1, 1)"/>
							<DOTATeamImage id="Team14Logo" class="TeamLogo" teamid="8255756" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8255756, 2, 1)"/>
							<DOTATeamImage id="Team15Logo" class="TeamLogo" teamid="2586976" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(2586976, 3, 1)"/>
						</Panel>
						<Panel class="TeamsRow LeftRightFlow">
							<DOTATeamImage id="Team16Logo" class="TeamLogo" teamid="7119388" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(7119388, 4, 1)"/>
							<DOTATeamImage id="Team17Logo" class="TeamLogo" teamid="8204512" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(8204512, 5, 1)"/>
							<DOTATeamImage id="Team18Logo" class="TeamLogo" teamid="350190" teamimagestyle="squarelarge" onactivate="DOTAShowDPCTeamPage(350190, 6, 1)"/>
						</Panel>
					</Panel>

				</Panel>

				<DOTACountdown id="Countdown" class="TopBottomFlow" end-time="1633590000" server-time="true">
					<Label class="CountdownHeader" text="#DOTA_Compendium_Countdown_Header" />
					<Label class="CountdownMain" text="#DOTA_Compendium_Countdown_Main" />
					<Panel class="CountdownDivider FillWidth" />
					<Label class="CountdownTimer" text="#DOTA_Compendium_Countdown_Timer" />
					<Label class="CountdownFinished" text="#DOTA_Compendium_Countdown_Finished" />
					
					<Panel class="PrizePoolPanel">
						<Label class="InfoSubText" text="#DOTA_DPC_PrizePool"/>
						<Label class="InfoMainText PrizePool" html="true" text="#DOTA_Compendium_Prizepool_2021"/>
					</Panel>

					<Panel class="VenuePanel">
							<Label class="InfoSubText" text="#DOTA_CompendiumRecap_OverviewVenue"/>
							<Label class="InfoMainText Location" html="true" text="#DOTA_Compendium_Location"/>
					</Panel>

				</DOTACountdown>
			</Panel>
		</Panel>
		

		<Panel class="ContentsSection CellsContainer">

			<DOTAFrontpageDefaultButton	class="FrontpageCell CompTI10Cell FirstCell"
				cellheader="#DOTA_Frontpage_CompendiumUpdate_Header"
				celltitle="#DOTA_Frontpage_CompendiumUpdate_Title"
				celltext="#DOTA_Frontpage_CompendiumUpdate_Desc"
				cellimage="s2r://panorama/images/front_page/ti10_news_01_psd.vtex"
				onactivate="BrowserGoToURL( 'https://www.dota2.com/newsentry/2870472829293831072' )" >
			</DOTAFrontpageDefaultButton>

			<DOTAFrontpageDefaultButton	class="FrontpageCell CompTI10Cell"
				cellheader="#DOTA_Frontpage_TI10Autographs_Header"
				celltitle="#DOTA_Frontpage_TI10Autographs_Title"
				celltext="#DOTA_Frontpage_TI10Autographs_Desc"
				cellimage="s2r://panorama/images/front_page/ti10_signatures_psd.vtex"
				onactivate="DOTAShowCompTI10CastersPage()" >
			</DOTAFrontpageDefaultButton>

			<DOTAFrontpageDefaultButton	class="FrontpageCell CompTI10Cell"
				cellheader="#DOTA_Frontpage_SupportersClubSale_Header"
				celltitle="#DOTA_Frontpage_SupportersClubSale_Title"
				celltext="#DOTA_Frontpage_SupportersClubSale_DescAlt"
				cellimage="s2r://panorama/images/front_page/ti10_dpc_supclub_sale_psd.vtex"
				onactivate="DOTAShowWatchDPCFanContentPage()" >
			</DOTAFrontpageDefaultButton>

			<DOTAFrontpageDefaultButton	class="FrontpageCell CompTI10Cell"
				cellheader="#DOTA_Frontpage_TI10ShortFilmContest_Header"
				celltitle="#DOTA_Frontpage_TI10ShortFilmContest_Title"
				celltext="#DOTA_Frontpage_TI10ShortFilmContest_Desc"
				cellimage="s2r://panorama/images/front_page/ti10_film_cell_psd.vtex"
				onactivate="DOTAShowShortFilmVote()" >
			</DOTAFrontpageDefaultButton>

			<!--
			                                                                                          
				                                              
				                                                     
				                                              
			                        
			-->
			
		</Panel>
	</CustomLayoutPanel>
</root>
 