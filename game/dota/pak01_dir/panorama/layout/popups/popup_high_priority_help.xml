<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/popups/popups_shared.vcss_c" />
		<include src="s2r://panorama/styles/popups/popup_high_priority_help.vcss_c" />
	</styles>

	<Panel class="PopupPanel Hidden" popupbackground="blur" oncancel="UIPopupButtonClicked()">
		<Panel id="Contents">
			<Panel class="HPMH">
				<Label class="HPMH_Title" text="#DOTA_HPQ_Help_Title" />
				<Label class="HPMH_Description" text="#DOTA_HPQ_Help_Description" />
				
				<Panel class="HPMH_Contents">
					<Panel class="HPMH_Contents_Column">
						<Panel class="HPMH_Example_Pad" />
						<Image class="HPMH_Example" src="s2r://panorama/images/leaf_pages/popup_high_priority_help/queue_all_roles_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
						<Label class="HPMH_ExampleTitle" text="#DOTA_HPQ_Help_EarnIt_Title" />
						<Label class="HPMH_ExampleDesc" text="#DOTA_HPQ_Help_EarnIt_Desc" />
					</Panel>
					<Panel class="HPMH_Contents_Column">
						<Panel class="HPMH_Example_Pad" />
						<Image class="HPMH_Example" src="s2r://panorama/images/leaf_pages/popup_high_priority_help/queue_any_role_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
						<Label class="HPMH_ExampleTitle" text="#DOTA_HPQ_Help_QueueFast_Title" />
						<Label class="HPMH_ExampleDesc" text="#DOTA_HPQ_Help_QueueFast_Desc" />
					</Panel>
					<Panel class="HPMH_Contents_Column">
						<Panel class="HPMH_Example_Pad" />
						<Image class="HPMH_Example" src="s2r://panorama/images/leaf_pages/popup_high_priority_help/high_priority_party_png.vtex" scaling="stretch-to-fit-preserve-aspect" />
						<Label class="HPMH_ExampleTitle" text="#DOTA_HPQ_Help_PartyUp_Title" />
						<Label class="HPMH_ExampleDesc" text="#DOTA_HPQ_Help_PartyUp_Desc" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
	</Panel>
</root>